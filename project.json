{
    "webroot": "wwwroot",
    "version": "1.0.0-*",

  "dependencies": {
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-beta8",
    "Microsoft.AspNet.IISPlatformHandler": "1.0.0-beta8",
    "Microsoft.AspNet.Diagnostics": "1.0.0-beta8",
    "Microsoft.Owin": "3.0.1",
    "Microsoft.AspNet.DataProtection": "1.0.0-beta8",
    "Microsoft.AspNet.Owin": "1.0.0-beta8",
    "IdentityServer3": "2.0.2",
    "Microsoft.Owin.StaticFiles": "3.0.1",
    "Microsoft.AspNet.Mvc": "6.0.0-beta8"
  },

    "commands": {
      "web": "Microsoft.AspNet.Server.Kestrel",
        "host": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls https://+:443;http://+:8080",
        "kestrel": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.Kestrel --server.urls https://+:443;http://+:8080"
    },

    "frameworks": {
        "dnx451": { }
    },

    "publishExclude": [
        "node_modules",
        "bower_components",
        "**.xproj",
        "**.user",
        "**.vspscc"
    ],
    "exclude": [
        "wwwroot",
        "node_modules",
        "bower_components"
    ]
}
