@using Fsw.Enterprise.AuthCentral.Areas.Admin.Models
@model ClientClaimContainer

@{
    Layout = "~/Areas/Admin/Views/Shared/_ClientAdminLayout.cshtml";
    ViewBag.Title = "Create Client Claim";
}

@using (Html.BeginForm("Save", "ClientClaim", new {clientId = Model.ClientId, area = "Admin" }, FormMethod.Post, new { @class = "form-horizontal"} )) {
    @Html.AntiForgeryToken()
    @Html.HiddenFor(model => model.ClientId)
    @Html.EditorFor(model=> model.ClientClaims)

    <div class="form-group">
       <div class="col-md-offset-2 col-md-10">
           <input type="submit" value="Save" class="btn btn-success" />
       </div>
    </div>
 }

<hr />
