@using Fsw.Enterprise.AuthCentral.Areas.UserAccount.Models
@model ChangeEmailRequestInputModel
@{
    ViewBag.Title = "Change Email";
}

<h2>@ViewBag.Title</h2>

@using (Html.BeginForm())
{
    @Html.ValidationSummary("Error")
    @Html.AntiForgeryToken()

    <fieldset>
        <legend>@ViewBag.Title</legend>

        @Html.EditorForModel()

        <div class="buttons">
            <button>Change Email</button>
        </div>
    </fieldset>
}
