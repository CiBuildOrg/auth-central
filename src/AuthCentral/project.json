{
    "name": "AuthCentral",
    "webroot": "wwwroot",
    "version": "1.0.0-*",

    "dependencies": {
      "AntiXSS": "4.3.0",
      "BrockAllen.MembershipReboot": "8.4.0",
      "FluentScheduler": "3.1.46",
      "IdentityServer3": "2.0.2",
      "IdentityServer3.MembershipReboot": "2.0.0",
      "IdentityServer3.MongoDb": "2.0.0",
      "IdentityServer3.MongoDb.Admin": "3.0.1",
      "Microsoft.AspNet.DataProtection": "1.0.0-beta8",
      "Microsoft.AspNet.Diagnostics": "1.0.0-beta8",
      "Microsoft.AspNet.IISPlatformHandler": "1.0.0-beta8",
      "Microsoft.AspNet.Mvc": "6.0.0-beta8",
      "Microsoft.AspNet.Owin": "1.0.0-beta8",
      "Microsoft.AspNet.Server.Kestrel": "1.0.0-beta8",
      "Microsoft.AspNet.Server.WebListener": "1.0.0-beta8",
      "Microsoft.AspNet.Authentication.Cookies": "1.0.0-beta8",
      "Microsoft.AspNet.Authentication.OpenIdConnect": "1.0.0-beta8",
      "Microsoft.Owin": "3.0.1",
      "Serilog": "1.5.12",
      "System.IdentityModel.Tokens.Jwt": "5.0.0-beta8-210131116",
      "System.Net.Http": "4.0.1-beta-23409"
    },

    "commands": {
      "host": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls https://+:44333;http://+:8080",
      "kestrel": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.Kestrel --server.urls https://+:44333;http://+:8080"
    },

    "frameworks": {
      "dnx451": {
        "dependencies": {
          "AuthCentral.MongoDb": "1.0.0-*"
        }
      }
    },

    "publishExclude": [
        "node_modules",
        "bower_components",
        "**.xproj",
        "**.user",
        "**.vspscc"
    ],
    "exclude": [
        "wwwroot",
        "node_modules",
        "bower_components"
    ]
}
